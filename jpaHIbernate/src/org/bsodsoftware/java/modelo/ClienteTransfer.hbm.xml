<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.bsodsoftware.java.modelo">
	
	<class name="ClienteTransfer" table="clientes">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="DNI" column="DNI"/>
		<property name="nombre" column="nombre"/>
		<property name="telefono" column="telefono"/>
		<property name="direccion" column="direccion"/>
	</class>
	
</hibernate-mapping>